{
  "name": "buybinds",
  "version": "3.1.2",
  "description": "CS GO buy binds keyboard mapper",
  "main": "public/index.html",
  "targets": {
    "main": {
      "sourceMap": false,
      "optimize": true,
      "includeNodeModules": {
        "react": false,
        "react-dom": false
      }
    }
  },
  "scripts": {
    "lint": "npx eslint .",
    "build": "parcel build ./src/index.html",
    "start": "parcel serve ./src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nksfrank/buybinds.git"
  },
  "keywords": [
    "CS",
    "GO",
    "counter",
    "strike",
    "buy",
    "binds"
  ],
  "author": "nksfrank",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nksfrank/buybinds/issues"
  },
  "homepage": "https://github.com/nksfrank/buybinds#readme",
  "devDependencies": {
    "@types/react": "~17.0.3",
    "@types/react-dom": "~17.0.3",
    "eslint": "7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react": "^1.1.7",
    "husky": "^6.0.0",
    "parcel": "^2.0.0-beta.2",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "dependencies": {
    "clsx": "1.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && pretty-quick --staged"
    }
  },
  "browserslist": [
    "last 1 version",
    ">1%",
    "IE 10",
    "not dead"
  ],
  "parcel": {
    "alias": {
      "react": "React",
      "react-dom": "ReactDOM"
    }
  }
}
